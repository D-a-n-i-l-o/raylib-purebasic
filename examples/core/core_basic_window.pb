IncludePath "./../../"XIncludeFile "raylib.pbi"UseModule ray#SCREEN_WIDTH = 800#SCREEN_HEIGHT = 450InitWindow( #SCREEN_WIDTH, #SCREEN_HEIGHT, "raylib [core] example - basic window" )If Not IsWindowReady() ; Error at init!    EndEndIfSetTargetFPS(60)While Not WindowShouldClose()        BeginDrawing()        ClearBackground(#COLOR_RAYWHITE)                DrawTextRaylib("Congrats! You created your first window!", 190, 200, 20, #COLOR_LIGHTGRAY)                DrawFPS(10,10)    EndDrawing()    If IsKeyPressed(#KEY_F1)        TakeScreenshot("screenshots/core_basic_window.png")    EndIfWendCloseWindowRaylib()